{
  "name": "hireable",
  "version": "0.3.0-rc.2",
  "description": "Available-for-hire badge",
  "main": "lib/index.js",
  "scripts": {
    "dev": "nodemon src/index.js --exec 'npm run lint && babel-node'",
    "test": "mocha --compilers js:babel-register",
    "test-live": "nodemon test --exec 'npm run lint:test && npm test'",
    "lint": "eslint --ext .js src build && npm run lint:test",
    "lint:test": "eslint --ext .js test",
    "serve": "node lib/index.js",
    "build": "shx rm -rf lib && babel src -d lib && shx cp -r src/styles lib/. && node build/build.js"
  },
  "private": true,
  "author": "Hien Dao Vinh <hien.dv.neo@gmail.com>",
  "contributors": [
    "Peter Nguyen <nguyenph88@gmail.com>",
    "Manh Tuan <junookyo@gmail.com>"
  ],
  "license": "MIT",
  "engineStrict": true,
  "engines": {
    "node": ">=4"
  },
  "dependencies": {
    "babel-runtime": "^6.11.6",
    "dotenv": "^2.0.0",
    "memoizee": "^0.4.1",
    "octokat": "^0.4.18",
    "send": "^0.14.1",
    "url-pattern": "^1.0.1"
  },
  "devDependencies": {
    "archiver": "^1.0.1",
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-register": "^6.11.6",
    "eslint": "^3.3.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "mocha": "^3.0.2",
    "muk": "^0.5.2",
    "nodemon": "^1.10.0",
    "shx": "^0.1.4"
  }
}
